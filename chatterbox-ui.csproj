<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>chatterbox_ui</RootNamespace>
    <AssemblyName>$(AssemblyName.Replace(' ', '_'))</AssemblyName>
    <BlazorDisableThrowNavigationException>true</BlazorDisableThrowNavigationException>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetEnv" Version="3.1.1" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="lib/chatterbox-inference-client-csharp-bindings/**/*.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="lib/chatterbox-inference-client-csharp-bindings/chatterbox-inference-client-csharp-bindings.csproj" />
  </ItemGroup>
</Project>
